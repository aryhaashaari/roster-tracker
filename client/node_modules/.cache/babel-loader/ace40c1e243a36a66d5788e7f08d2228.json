};\n        stats.value = [];\n        setTimeout (() => {\n          statusMsg.value = false;\n        }, 5000);\n      } catch (error) {\n        errorMsg.value = `Error sending stats: ${error.message}`;\n          setTimeout(() => {\n          errorMsg.value = false;\n        }, 5000);\n      }\n    };\n\n    return {player_name, position, physique, stats, statusMsg, errorMsg, addStats, deleteStats, positionChange, createPlayer};\n  },\n};\n</script>"],"mappings":";OAyGqDA,UAA+B;;EAxG7EC,KAAK,EAAC;AAAoC;;;EAEXA,KAAK,EAAC;;;EACnCA,KAAK,EAAC;AAAgB;;EAGtBA,KAAK,EAAC;AAAc;;EAMpBA,KAAK,EAAC;AAAmD;gCAG1DC,mBAAA,CAA+C;EAA3CD,KAAK,EAAC;AAAqB,GAAC,YAAU;;EAGrCA,KAAK,EAAC;AAAe;gCACxBC,mBAAA,CAA4E;EAArEC,GAAG,EAAC,aAAa;EAACF,KAAK,EAAC;GAA0B,aAAW;;EAKjEA,KAAK,EAAC;AAAe;iCACxBC,mBAAA,CAA6E;EAAtEC,GAAG,EAAC,UAAU;EAACF,KAAK,EAAC;GAA0B,iBAAe;iCAEnEC,mBAAA,CAAgC,gBAAxB,iBAAe;iCACvBA,mBAAA,CAAgC;EAAxBE,KAAK,EAAC;AAAG,GAAC,OAAK;iCACvBF,mBAAA,CAAkC;EAA1BE,KAAK,EAAC;AAAG,GAAC,SAAO;iCACzBF,mBAAA,CAAiC;EAAzBE,KAAK,EAAC;AAAG,GAAC,QAAM;qBAHxBC,WAAgC,EAChCC,WAAgC,EAChCC,WAAkC,EAClCC,WAAiC,C;;;EAKgCP,KAAK,EAAC;;;EACpEA,KAAK,EAAC;AAAoD;;EAExDA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAAuE;EAAhEC,GAAG,EAAC,QAAQ;EAACF,KAAK,EAAC;GAA0B,aAAW;;EAG5DA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAAuE;EAAhEC,GAAG,EAAC,QAAQ;EAACF,KAAK,EAAC;GAA0B,aAAW;;EAG5DA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAA4D;EAArDC,GAAG,EAAC,KAAK;EAACF,KAAK,EAAC;GAA0B,KAAG;;EAGjDA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAA2E;EAApEC,GAAG,EAAC,UAAU;EAACF,KAAK,EAAC;GAA0B,eAAa;;;EAOJA,KAAK,EAAC;;;EAMpEA,KAAK,EAAC;AAAuB;;EAC3BA,KAAK,EAAC;AAAuB;;EAC3BA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAAkE;EAA3DC,GAAG,EAAC,QAAQ;EAACF,KAAK,EAAC;GAA0B,QAAM;;;EAGvDA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAAoE;EAA7DC,GAAG,EAAC,SAAS;EAACF,KAAK,EAAC;GAA0B,SAAO;;;EAGzDA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAAsE;EAA/DC,GAAG,EAAC,UAAU;EAACF,KAAK,EAAC;GAA0B,UAAQ;;;EAG3DA,KAAK,EAAC;AAAsB;iCAC/BC,mBAAA,CAAqE;EAA9DC,GAAG,EAAC,cAAc;EAACF,KAAK,EAAC;GAA0B,KAAG;;;EAM5DA,KAAK,EAAC;AAAuB;;EAC3BA,KAAK,EAAC;AAAsB;iCAC/BC,m