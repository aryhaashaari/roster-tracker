 hover:border-black hover:bg-white hover:text-black\">\n        Add Player\n        </router-link>\n       </div>\n  \n      <!-- Data -->\n      <div v-else class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-6\">\n        <router-link class=\"flex flex-col items-center bg-light-grey p-8 shadow-md cursor-pointer\"\n        :to=\"{name: 'View-Player', params: {playerId: player.id}}\"\n        v-for=\"(player, index) in data\"\n        :key=\"index\"\n        >\n          <!-- Player Card -->\n          <img src=\"@/assets/images/person.png\" class=\"h-24 w-auto\" alt=\"\"/>\n  \n          <p class=\"mt-6 py-1 px-3 text-xs text-white bg-black shadow-md rounded-lg\">\n            {{player.position}}\n          </p>\n  \n          <h1 class=\"mt-8 mb-2 text-center text-xl text-black\">\n            {{player.playerName}}\n          </h1>\n        </router-link>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import { ref } from \"vue\";\n  import { supabase } from \"../supabase/init\";\n  export default {\n    name: \"Home\",\n    components: {},\n    setup() {\n      // Create data / vars\n      const data = ref([]);\n      const dataLoaded = ref(null);\n  \n      // Get data\n      const getData = async () => {\n        try {\n          const {data: players, error} = await supabase.from('players').select('*');\n          if (error) throw error;\n          data.value = players;\n          dataLoaded.value = true;\n        } catch (error) {\n          console.warn(error.message);\n        }\n      }\n  \n      // Run data function\n      getData();\n  \n      return {data, dataLoaded};\n    },\n  };\n  </script>"]}]}